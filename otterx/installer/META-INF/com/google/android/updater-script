assert(getprop("ro.product.device") == "otterx" || getprop("ro.build.product") == "otterx" || abort("This package is for \"otterx\" devices; this is a \"" + getprop("ro.product.device") + "\"."););
show_progress(0.500000, 0);
mount("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.1/by-name/system", "/system");
package_extract_dir("system", "/system");
set_perm_recursive(0, 0, 0755, 0644, "/system/lib/modules");
unmount("/system");
show_progress(0.200000, 10);
package_extract_file("boot.img", "/dev/block/platform/omap/omap_hsmmc.1/by-name/boot");
show_progress(0.100000, 0);
